{
    "name": "sudippalash/mediauploader",
    "description": "Laravel package for Media Uploader",
    "keywords": [
        "Sudip",
        "Laravel Media Uploader",
        "Media Uploader",
        "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sudip Palash",
            "email": "palash.sudip@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^7.0|8.0|^9.0|^10.0|^11.0",
        "intervention/image": "^2.5"
    },
    "autoload": {
        "psr-4": {
          "Sudip\\MediaUploader\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sudip\\MediaUploader\\MediaUploaderServiceProvider"
            ],
            "aliases": {
                "MediaUploader": "Sudip\\MediaUploader\\Facades\\MediaUploader"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
